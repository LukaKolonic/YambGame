<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="hr.algebra.controller.MenuController">
   <children>
      <Label layoutX="80.0" layoutY="164.0" text="Nickname:">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TextField fx:id="tfNickname" layoutX="218.0" layoutY="164.0" onKeyReleased="#handleBtnPlay" prefHeight="38.0" prefWidth="231.0" />
      <Button fx:id="btnPlay" disable="true" layoutX="291.0" layoutY="302.0" mnemonicParsing="false" onAction="#goToGame" prefHeight="38.0" prefWidth="105.0" text="Play">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
